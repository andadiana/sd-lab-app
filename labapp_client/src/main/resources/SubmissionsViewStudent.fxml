<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="540.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SubmissionsViewStudent">
    <children>
        <TableView fx:id="submissionsTable" layoutX="20.0" layoutY="34.0" onMouseClicked="#submissionsTableClicked" prefHeight="210.0" prefWidth="580.0">
            <columns>
                <TableColumn fx:id="assignmentNameColumn" prefWidth="269.0" text="Assignment Name" />
                <TableColumn fx:id="dateColumn" prefWidth="225.0" text="Submission Date" />
                <TableColumn fx:id="gradeColumn" prefWidth="72.0" text="Grade" />
            </columns>
        </TableView>
        <ComboBox fx:id="assignmentComboBox" layoutX="126.0" layoutY="322.0" prefWidth="150.0" />
        <TextArea fx:id="descriptionTextArea" layoutX="26.0" layoutY="388.0" prefHeight="135.0" prefWidth="580.0" promptText="Submission description" wrapText="true" />
        <Button fx:id="submitButton" layoutX="299.0" layoutY="322.0" mnemonicParsing="false" onAction="#submitButtonClicked" text="New submission" />
        <Button fx:id="updateButton" layoutX="437.0" layoutY="322.0" mnemonicParsing="false" onAction="#updateButtonClicked" text="Update" />
        <Label fx:id="errorLabel" layoutX="44.0" layoutY="255.0" prefHeight="25.0" prefWidth="532.0" text="Label" />
      <Label layoutX="30.0" layoutY="325.0" text="Assignment" />
      <Label layoutX="30.0" layoutY="367.0" text="Description" />
    </children>
</AnchorPane>
